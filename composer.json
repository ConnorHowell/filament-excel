{
    "name": "pxlrbt/filament-excel",
    "description": "Supercharged Excel exports for Filament Resources",
    "license": "MIT",
    "keywords": [
        "laravel",
        "phpspreadsheet",
        "phpexcel",
        "filament",
        "laravel-filament",
        "actions"
    ],
    "authors": [
        {
            "name": "Dennis Koch",
            "email": "info@pixelarbeit.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.0|^3.0",
        "maatwebsite/excel": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "pxlrbt\\FilamentExcel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pxlrbt\\FilamentExcel\\FilamentExcelServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "style": [
            "@cs-fixer"
        ]
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    }
}
